{
    "image": "ghcr.io/rhel-yeah/ubi-nosub:rhel-8",
    "remoteUser": "vscode",
    "features": {
		"ghcr.io/devcontainers/features/common-utils:2": {
			"installZsh": "true",
			"username": "vscode",
			"userUid": "1000",
			"userGid": "1000",
			"upgradePackages": "true"
		},
		"ghcr.io/rhel-yeah/features/gh-cli":{}
	},
    "privileged":true,
	"postCreateCommand": "sudo sed -i 's/enabled=1/enabled=0/' /etc/yum/pluginconf.d/subscription-manager.conf"
}